digraph {
    node [shape = "box", style = "filled, rounded"]

    subgraph clusterorigin {
        label = "origin"

        node [color = orchid]

        omain[label = "main"]
    }

    subgraph clusterlocal {
        label = "local"

        node [color = orchid]

        main[label = "main"]
    }

    omain -> main [label = "git fetch", style = dotted]
    main -> omain [label = "git push"]
    omain -> main [label = "git pull"]
}
